RewriteEngine on



RewriteCond %{REQUEST_URI} ^system.*
RewriteRule ^(.*)$ /index.php/$1 [L]

RedirectMatch 301 /us.php /view/us-web/track
RedirectMatch 301 /uk.php /view/uk-web/track
RedirectMatch 301 /ca.php /view/ca-web/track

RewriteCond $1 !^(index\.php|images|robots\.txt|us\.php|uk\.php|ca\.php)
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php/$1 [L]

ErrorDocument 404 /index.php